{
  "name": "WestChart_ReactApp",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1",
    "yarn": "1.3.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "concurrently": "^3.5.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "7.7.0",
    "prettier": "^1.11.1"
  },
  "scripts": {
    "start:dev_srv": "concurrently --prefix \"[{name}]\" -c \"green.dim,blue.italic\" --names \"Client,Server\" \"npm run start:dev\" \"npm run start:srv\"",
    "start:dev": "cd web && yarn start",
    "start": "node server",
    "lint": "eslint \"**/*.@(js|jsx)\"",
    "format": "npm run lint -- --fix",
    "start:srv": "cd server && yarn start:srv",
    "build": "node web/scripts/build.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react-app"
    ]
  }
}
